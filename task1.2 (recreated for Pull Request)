odd_numbers = list(range(1, 1000, 2))
cubed_numbers = [i**3 for i in odd_numbers]


def multiplicity(cubed_list: list) -> list:
    """Принимает на вход список кубов, а возвращает сумму кубов,сумма цифр которых кратна 7"""

    finale = 0

    for num in cubed_list:
        summ_digits = 0
        nums_ = num

        while nums_ > 0:
            summ_digits += nums_ % 10
            nums_ //= 10

        if summ_digits % 7 == 0:
            finale += num

    return finale


print(multiplicity(cubed_numbers))


def multiplicity(cubed_list: list) -> list:
    """Принимает на вход список кубов + 17, а возвращает сумму кубов,сумма цифр которых кратна 7"""

    finale = 0

    for num in cubed_list:
        summ_digits = 0
        nums_ = num + 17

        while nums_ > 0:
            summ_digits += nums_ % 10
            nums_ //= 10

        if summ_digits % 7 == 0:
            finale += num

    return finale


print(multiplicity(cubed_numbers))
